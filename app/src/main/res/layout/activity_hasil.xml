<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginTop="10dp"
    android:padding="10dp"
    android:id="@+id/hasil_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ijul.cekpajak.HasilActivity">

    <TextView
        android:id="@+id/plat"
        android:layout_centerHorizontal="true"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/datakendaraan"
        android:textStyle="bold"
        android:layout_below="@id/plat"
        android:layout_marginBottom="10dp"
        android:text="DATA KENDARAAN :"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tprovinsi"
        android:textStyle="bold"
        android:text="Provinsi"
        android:layout_below="@id/datakendaraan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tjenis"
        android:textStyle="bold"
        android:text="Jenis Kendaraan"
        android:layout_below="@id/tprovinsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tmerk"
        android:textStyle="bold"
        android:text="Merk Kendaraan"
        android:layout_below="@id/tjenis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/ttype"
        android:textStyle="bold"
        android:text="Tipe Kendaraan"
        android:layout_below="@id/tmerk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/ttahun"
        android:textStyle="bold"
        android:text="Tahun Pembuatan"
        android:layout_below="@id/ttype"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/twarna"
        android:text="Warna Kendaraan"
        android:textStyle="bold"
        android:layout_below="@id/ttahun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/twilayah"
        android:text="Wilayah"
        android:textStyle="bold"
        android:layout_below="@id/twarna"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tpkb"
        android:text="PAJAK KENDARAAN"
        android:textStyle="bold"
        android:layout_below="@id/twilayah"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tjumlah"
        android:text="Jumlah"
        android:textStyle="bold"
        android:layout_below="@id/tpkb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tjatuh"
        android:textStyle="bold"
        android:text="Jatuh Tempo"
        android:layout_below="@id/tjumlah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/tbiaya"
        android:text="BIAYA :"
        android:textStyle="bold"
        android:layout_below="@id/tjatuh"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tbaliknama"
        android:text="Balik Nama"
        android:textStyle="bold"
        android:layout_below="@id/tbiaya"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tjasaraharja"
        android:textStyle="bold"
        android:text="Jasa Raharja"
        android:layout_below="@id/tbaliknama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <!--KANAN-->
    <TextView
        android:id="@+id/zplat"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/kdatakendaraan"
        android:textStyle="bold"
        android:layout_below="@id/zplat"
        android:layout_alignParentRight="true"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/provinsi"
        android:layout_below="@id/kdatakendaraan"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/jenis"
        android:layout_alignParentRight="true"
        android:layout_below="@id/provinsi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/merk"
        android:layout_alignParentRight="true"
        android:layout_below="@id/jenis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/type"
        android:layout_alignParentRight="true"
        android:layout_below="@id/merk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tahun"
        android:layout_alignParentRight="true"
        android:layout_below="@id/type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/warna"
        android:layout_alignParentRight="true"
        android:layout_below="@id/tahun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/wilayah"
        android:layout_alignParentRight="true"
        android:layout_below="@id/warna"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/pkb"
        android:layout_alignParentRight="true"
        android:layout_below="@id/wilayah"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/jumlah"
        android:layout_alignParentRight="true"
        android:layout_below="@id/pkb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/jatuh"
        android:layout_alignParentRight="true"
        android:layout_below="@id/jumlah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/biaya"
        android:layout_alignParentRight="true"
        android:layout_below="@id/jatuh"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/baliknama"
        android:layout_alignParentRight="true"
        android:layout_below="@id/biaya"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/jasaraharja"
        android:layout_alignParentRight="true"
        android:layout_below="@id/baliknama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>